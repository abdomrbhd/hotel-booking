+section("subscribe position-relative")
    .subscribe__inner.position-relative
        .sub-wrap.position-relative
            .container
                .row
                    .subscribe-content.col-8.offset-2.d-flex.justify-content-between.align-items-end.pt-4.pb-4
                        .title
                            h3.mb-0 subscribe to our
                                br
                                span.text-uppercase newsletter
                        .search
                            .search_box
                                .search-wrap.m-auto.d-flex
                                    input(type="email" placeholder="Your Email Address")
                                    button.button.button-pr send
    .overllay.position-absolute.w-100.h-100